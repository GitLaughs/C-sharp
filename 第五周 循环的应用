 using System;

class Program
{
    static void Main(string[] args)
    {
        string[] tokens = Console.ReadLine().Split();
        int a = int.Parse(tokens[0]);
        int n = int.Parse(tokens[1]);
        long sum = 0;
        long number = 0;

        for (int i = 0; i < n; i++)
        {
            number = number * 10 + a;
            sum += number;
        }

        Console.WriteLine("{0}",sum);
    }
}
